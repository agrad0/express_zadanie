<div class="container">
    <h2>Users table</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Address</th>
                <th scope="col">Personal website</th>
            </tr>
        </thead>
        <tbody>
            {{#each users as |user|}}
            <tr>
                <th scope="row">1</th>
                <td>{{user.name}}</td>
                <td>{{user.address.street}}</td>
                <td>Website link</td>
            </tr>
            {{/each}}

        </tbody>
    </table>
</div>